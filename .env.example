# Weather API (Get key from https://www.weatherapi.com/)
WEATHER_API_KEY=your_weather_api_key_here

# OpenRouter LLM (Get key from https://openrouter.ai/)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# S3 Bucket (Optional for local dev, required for full feature)
WEATHER_BUCKET_NAME=fitted-weather-data-local
# Set to true to disable S3 uploads during local development
IS_LOCAL=true